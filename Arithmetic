#include <iostream>
using namespace std ;
int main ()
{
int a,b;
double add;
double sub;
double div;
double multi;
cout << "Enter two digit value" << endl;
cin >> a >> b;
add = a+b;
sub = a-b;
div = a/b;
multi = a*b;
cout << "addition of two number is" << add << endl;
cout << "Subtraction of two number is" <<  sub << endl;
cout << "Division of two number is" << div << endl;
cout << "Multiplication of two number is"<< multi << endl;
return 0;
}
